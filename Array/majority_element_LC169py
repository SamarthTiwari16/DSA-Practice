class Solution:
    def majorityElement(self, nums: List[int]) -> int:
        n = len(nums)
        cand = None
        count = 0
        for i in nums:
            if count == 0:
                cand = i
                count += 1
            elif cand == i:
                count += 1
            else:
                count -= 1
        count = 0
        for i in nums:
            if cand == i:
                count+=1
        if count > n//2:
            return cand
        else:
            return -1
